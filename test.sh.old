#!/bin/bash

##rm test_sh.log
#redirection of stdout and stderr
# Close standard output file descriptor
exec 1<&-
# Close standard error file descriptor
exec 2<&-
# Open standard output as $LOG_FILE file for read and write.
exec 1<>test_sh.log.qwq32
# Redirect standard error to standard output
exec 2>&1

#loading venv
. .venv/bin/activate

#cleaning results
##rm *.result

#functions
call_qwq_llm () {
    LLM="/llm/QwQ32/"
    echo "calling "$LLM
    python test2.py $LLM "8000" $1.problem $1.solution $1.result
}
call_qwen3_llm () {
    LLM="/llm/Qwen3-32B"
    echo "calling "$LLM
    python test2.py $LLM "8001" $1.problem $1.solution $1.result
}
call_rtl_llm () {
    LLM="/llm/SakanaAI-RTL-32"
    echo "calling "$LLM
    python test2.py $LLM "8003" $1.problem $1.solution $1.result
}

start_question () {
    echo "---------- question:"$1
    call_qwq_llm $1
    #call_qwen3_llm $1
    #call_rtl_llm $1
}

#main

date
